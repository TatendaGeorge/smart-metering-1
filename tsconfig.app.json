/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "paths": {
    "@pages/*": [
      "src/app/pages/*"
    ],
    "@shared/*": [
      "src/app/shared/*"
    ]
  },
  "include": [
    "src/**/*.d.ts"
  ]
}
